{
  "name": "dash-core-components",
  "version": "0.1.4",
  "description": "Core component suite for Dash2",
  "repository": {
    "type": "git",
    "url": "git://github.com/plotly/dash-core-components.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "copy-metadata": "cp lib/metadata.json dash_core_components/",
    "demo": "builder run demo",
    "install-local": "npm run prepublish && python setup.py install",
    "prepublish": "npm test && builder run transpile-dev && npm run copy-metadata",
    "publish-all": "npm publish && python setup.py sdist upload",
    "publish-pypi": "npm run prepublish && python setup.py sdist upload",
    "start": "builder run transpile-dev-watch",
    "test": "builder run lint",
    "uninstall-local": "pip uninstall dash-core-components -y"
  },
  "author": "Per Nilsson <per@sproutlab.com>",
  "license": "MIT",
  "dependencies": {
    "builder": "2.10.1",
    "dash-components-archetype": "^0.1.7",
    "plotly.js": "^1.14.2",
    "radium": "^0.17.1",
    "ramda": "^0.21.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "dash-components-archetype-dev": "^0.1.7"
  }
}
